{
  "name": "delta",
  "version": "1.10.55",
  "scripts": {
    "ng": "ng",
    "start:ng": "ng serve",
    "build": "ng build && node ./node_modules/typescript/lib/tsc ./server.ts --target es6 --types node --module commonjs --sourcemap && node ./node_modules/typescript/lib/tsc ./src/server/MoInstallSQL.test.ts --target es6 --types node,mocha --module commonjs --sourcemap",
    "build:prod": "ng build --prod && node ./node_modules/typescript/lib/tsc ./server.ts --target es6 --types node --module commonjs --sourcemap && node ./node_modules/typescript/lib/tsc ./src/server/MoInstallSQL.test.ts --target es6 --types node,mocha --module commonjs --sourcemap",
    "start": "node --inspect server.js",
    "test": "ng test",
    "coverage:node": "node ./node_modules/nyc/bin/nyc npm run test:node",
    "lint": "ng lint",
    "prettier": "node ./node_modules/prettier/bin-prettier.js --write ./src/**/*.ts",
    "e2e": "ng e2e",
    "test:node": "node ./node_modules/mocha/bin/mocha",
    "bstart": "npm run build && npm start",
    "prod:start": "npm run build:prod && npm start",
    "dev": "nodemon",
    "open:coverage": "start ./coverage/index.html"
  },
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "instrument": true,
    "sourceMap": true,
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ]
  },
  "author": "mycomplexsoul",
  "dependencies": {
    "@angular/animations": "10.2.0",
    "@angular/common": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/forms": "10.2.0",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/router": "10.2.0",
    "@angular/service-worker": "10.2.0",
    "@types/node": "12.19.2",
    "chart.js": "2.9.4",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "html2canvas": "^1.0.0-rc.5",
    "jsonwebtoken": "8.5.1",
    "jspdf": "^2.0.0",
    "mysql": "2.18.1",
    "ng2-charts": "2.4.2",
    "nodemailer": "6.4.14",
    "rxjs": "6.6.3",
    "tslib": "2.0.2",
    "typescript": "3.9.7",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.0",
    "@angular/language-service": "10.2.0",
    "@types/chart.js": "2.9.27",
    "@types/express": "4.17.8",
    "@types/jasmine": "3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.3",
    "chai": "4.2.0",
    "codelyzer": "6.0.1",
    "istanbul": "0.4.5",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "nyc": "15.1.0",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.0.0",
    "tslint": "6.1.3"
  },
  "prettier": {
    "tabWidth": 2
  }
}
