{
  "name": "delta",
  "version": "1.10.47",
  "scripts": {
    "ng": "ng",
    "start:ng": "ng serve",
    "build": "ng build && node ./node_modules/typescript/lib/tsc ./server.ts --target es6 --types node --module commonjs --sourcemap && node ./node_modules/typescript/lib/tsc ./src/server/MoInstallSQL.test.ts --target es6 --types node,mocha --module commonjs --sourcemap",
    "build:prod": "ng build --prod && node ./node_modules/typescript/lib/tsc ./server.ts --target es6 --types node --module commonjs --sourcemap && node ./node_modules/typescript/lib/tsc ./src/server/MoInstallSQL.test.ts --target es6 --types node,mocha --module commonjs --sourcemap",
    "start": "node --inspect server.js",
    "test": "ng test",
    "coverage:node": "node ./node_modules/nyc/bin/nyc npm run test:node",
    "lint": "ng lint",
    "prettier": "node ./node_modules/prettier/bin-prettier.js --write ./src/**/*.ts",
    "e2e": "ng e2e",
    "test:node": "node ./node_modules/mocha/bin/mocha",
    "bstart": "npm run build && npm start",
    "prod:start": "npm run build:prod && npm start",
    "dev": "nodemon",
    "open:coverage": "start ./coverage/index.html"
  },
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "instrument": true,
    "sourceMap": true,
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ]
  },
  "author": "mycomplexsoul",
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.901.7",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@types/node": "~8.9.4",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.16.0",
    "ng2-charts": "^2.3.2",
    "nodemailer": "^5.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^1.9.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.28",
    "@angular/cli": "~8.3.28",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/chart.js": "^2.7.51",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "codelyzer": "^5.0.1",
    "istanbul": "^0.4.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.3.0",
    "tslint": "~5.11.0"
  },
  "prettier": {
    "tabWidth": 2
  }
}
